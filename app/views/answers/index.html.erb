<%- model_class = Answer-%>
  <div class="page-header">
    <h1>Repuestas</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>id</th>
          <th>Repuestas</th>
          <th>Preguntas</th>
          <th>Letras</th>
        </tr>
      </thead>
      <tbody>
       <%= render @answers %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_answer_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
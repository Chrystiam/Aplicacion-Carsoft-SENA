<%- model_class = AreaCenter -%>
<div class="page-header">
  <h1>Area Centro</h1>
  <div class="navbar">
    <div class="navbar-inner">
    <li id='buscar' class="brand" href="#">Buscar</li> 
      <div class="row">
        <div class="span6">
          <form method="GET" class="form-inline" >
            <input id='buscar' name="buscar" class="search-query" type="text"  placeholder="Buscar" >
           <a  rel="tooltip" title="Ingresar datos a buscar"><button id='buscar' type="submit" class="btn btn-primary btn-mini"><i class="icon-search icon-white"></i></button></a> 
          </form>
        </div>
      </div>
    </div>
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th class="fondo ">id</th>
      <th class="fondo ">Nombre</th>
      <th class="fondo ">Responsable</th>
      <th class="fondo ">Extension</th>
      <th class="fondo ">Telefono</th>
      <th class="fondo ">Especificacion</th>
      <th class="fondo ">Opciones</th>
      <th class="fondo "></th>
    </tr>
  </thead>
  <tbody>
    <%= render @area_centers %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to image_tag("participantes.png"), new_responsible_area_center_path, :remote => true  %> </td>
      <td><%= link_to image_tag("atras.png"), responsibles_path %></td>
    </tr>
  </tfoot>
</table>
<%- model_class = Category  -%>
	<div class="page-header">
  		<h1>Categoria</h1>
		<table class="table table-bordered table-hover" >
	    	<caption>
	    		<h3>Informacion</h3>
	    	</caption>
	    	<thead>
	    		<tr class="success">
	    			<th>Id</th>
	    			<th>Nombre</th>
	    		</tr>
	    	</thead>
	    	<tbody>
	    		<tr class="warning">
				    <td><p><%= @category.id %></p></td>
				    <td><p><%= @category.name %></p></td>
	    		</tr>
	    	</tbody>
	    </table>
	</div>

<%- model_class = CenterProgram -%>
<div class="page-header">
  <h1>Centros Programas</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>center_id</th>
      <th>user_id</th>
      <th>program_id</th>
      <th>created_at</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @center_programs.each do |center_program| %>
      <tr>
        <td><%= link_to center_program.id, center_program_path(center_program) %></td>
        <td><%= center_program.center_id %></td>
        <td><%= center_program.user_id %></td>
        <td><%= center_program.program_id %></td>
        <td><%=l center_program.created_at %></td>
        <td>
          <%= link_to 'Editar', edit_center_program_path(center_program), :class => 'btn btn-mini' %>
          <%= link_to 'Eliminar', center_program_path(center_program),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Nuevo Centro Programa', new_center_program_path, :class => 'btn btn-primary' %>

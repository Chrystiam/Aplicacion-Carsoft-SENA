<tr id=<%= "center_#{center.id}" %>>
    <td><%= link_to center.id, edit_center_path(center), :remote => true %></td>
    <td><%= center.name %></td>
    <td><%= center.code_id %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), center,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_center_path(center),  :remote => true,
          :class => 'btn btn-mini'%>
        <%= link_to image_tag('eliminar.png', :title => "Eliminar Equipos"), center, :data => { :confirm => 'Esta Seguro?' },
          :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
    </td>
</tr>
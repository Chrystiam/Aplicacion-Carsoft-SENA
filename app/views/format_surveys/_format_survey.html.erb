<tr id=<%= "format_survey_#{format_survey.id}" %>>
    <td><%= link_to format_survey.id, edit_format_survey_path(format_survey), :remote => true %></td>
    <td><%= format_survey.name %></td>
    <td><%= format_survey.user_id %></td>
    <td><%= format_survey.survey_id  %></td>
    <td><%= format_survey.answer_id %></td>
    <td><%= format_survey.question_id %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), format_survey,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_format_survey_path(format_survey),  :remote => true,
          :class => 'btn btn-mini'%>
        <%= link_to image_tag('eliminar.png', :title => "Eliminar Equipos"), format_survey, :data => { :confirm => 'Esta Seguro?' },
          :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
    </td>
</tr>
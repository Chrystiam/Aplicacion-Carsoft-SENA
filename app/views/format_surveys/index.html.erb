<%- model_class = FormatSurvey -%>
  <div class="page-header">
    <h1>Formato de Encuestas</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
            <th>Id</th>
            <th>Nombres</th>
            <th>Usuarios</th>
            <th>Estudios</th>
            <th>Repuestas</th>
            <th>Preguntas</th>
        </tr>
      </thead>
      <tbody>
       <%= render @format_surveys %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_format_survey_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
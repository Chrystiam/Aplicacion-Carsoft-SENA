<%- model_class = Material -%>
	<div class="page-header">
  		<h1>Materiales</h1>
		<table class="table table-bordered table-hover" >
	    	<caption>
	    		<h3>Informacion</h3>
	    	</caption>
	    	<thead>
	    		<tr class="success">
	    			<th>Id</th>
	    			<th>Nombres</th>
	    			<th>Tipo materiales</th>
	    			<th>Usuarios</th>
	    			<th>Descripcion</th>
	    			
	    		</tr>
	    	</thead>
	    	<tbody>
	    		<tr class="success">
				    <td><p><%= @material.id %></p></td>
				    <td><p><%= @material.name %></p></td>
				    <td><p><%= @material.type_material_id  %></p></td>
				    <td><p><%= @material.user_id %></p></td>
				    <td><p><%= @material.description   %></p></td>	    
	    		</tr>
	    	</tbody>
	    </table>
	</div>

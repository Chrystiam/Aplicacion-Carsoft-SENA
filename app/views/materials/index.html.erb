<%- model_class = Material -%>
  <div class="page-header">
    <h1>Materiales</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
            <th>Id</th>
            <th>Nombres</th>
            <th>Tipo materiales</th>
            <th>Usuarios</th>
            <th>Descripcion</th>
        </tr>
      </thead>
      <tbody>
       <%= render @materials %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_material_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
<tr id=<%= "modify_user_#{modify_user.id}" %>>
    <td><%= link_to modify_user.id, edit_user_modify_user_path(@user, modify_user), :remote => true %></td>
    <td><%= modify_user.change  %></td>
    <td><%= modify_user.description %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), user_modify_user_path(@user, modify_user),     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_user_modify_user_path(@user, modify_user),  :remote => true,
          :class => 'btn btn-mini'%>
        <%= link_to image_tag('eliminar.png', :title => "Eliminar Equipos"), user_modify_user_path(@user, modify_user), :data => { :confirm => 'Esta Seguro?' }, :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
    </td>
</tr>
<%- model_class = Parking -%>
  <div class="page-header">
    <h1>Aparcamientos</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>id</th>
          <th>espacio disponible</th>
          <th>Ubicacion</th>
          <th>Zonas</th>
          <th>Vehiculos</th>
          <th>Descripciones</th>
        </tr>
      </thead>
      <tbody>
       <%= render @parkings %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_parking_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
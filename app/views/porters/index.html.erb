<%- model_class = Porter -%>
  <div class="page-header">
    <h1>Porteros</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
            <th>Id</th>
            <th>Nombres</th>
            <th>Apellidos</th>
            <th>Direccion</th>
            <th>Telefono</th>
            <th>Turno</th>
        </tr>
      </thead>
      <tbody>
       <%= render @porters %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_porter_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
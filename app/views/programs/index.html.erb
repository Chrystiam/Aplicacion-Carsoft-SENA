<%- model_class = Program -%>
  <div class="page-header">
    <h1>Programas</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
            <th>id</th>
            <th>Nombre</th>
            <th>Centros</th>
            <th>descripcion</th>
        </tr>
      </thead>
      <tbody>
       <%= render @programs %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_program_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
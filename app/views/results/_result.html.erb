<tr id=<%= "result_#{result.id}" %>>
    <td><%= link_to result.id, edit_result_path(result), :remote => true %></td>
    <td><%= result.answer_id %></td>
    <td><%= result.question_id %></td>
    <td><%= result.user_id %></td>
    <td><%= result.ok %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), result,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_result_path(result),  :remote => true,
          :class => 'btn btn-mini'%>
        <%= link_to image_tag('eliminar.png', :title => "Eliminar Equipos"), result, :data => { :confirm => 'Esta Seguro?' },
          :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
    </td>
</tr>
<tr id=<%= "sheet_#{sheet.id}" %>>
    <td><%= link_to sheet.id, edit_sheet_path(sheet), :remote => true %></td>
    <td><%= sheet.code %></td>
    <td><%= sheet.program_id %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), sheet,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_sheet_path(sheet),  :remote => true,
          :class => 'btn btn-mini'%>
        <%= link_to image_tag('eliminar.png', :title => "Eliminar Equipos"), sheet, :data => { :confirm => 'Esta Seguro?' },
          :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
    </td>
</tr>
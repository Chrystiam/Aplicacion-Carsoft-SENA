<%- model_class = Survey -%>
	<div class="page-header">
  		<h1>Encuestas</h1>
		<table class="table table-bordered table-hover" >
	    	<caption>
	    		<h3>Informacion</h3>
	    	</caption>
	    	<thead>
	    		<tr class="success">
	    			<th>Id</th>
	    			<th>Usuarios</th>
	    			<th>Descripcion</th>
	    		</tr>
	    	</thead>
	    	<tbody>
	    		<tr class="warning">
				    <td><p><%= @survey.id %></p></td>
				    <td><p><%= @survey.user_id %></p></td>
				    <td><p><%= @survey.description %></p></td>   
	    		</tr>
	    	</tbody>
	    </table>
	</div>

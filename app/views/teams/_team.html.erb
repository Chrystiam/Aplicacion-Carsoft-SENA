<tr id=<%= "team_#{team.id}" %>>
    <td><%= link_to team.id, edit_user_team_path(@user, team), :remote => true %></td>
    <td><%= team.name %></td>
    <td><%= team.serial %></td>
    <td><%= team.mouse %></td>
    <td><%= team.charger %></td>
    <td><%= team.color %></td>
    <td><%= team.type_material.name %></td>
    <td><%= team.porter.name %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), user_team_path(@user, team),     :remote => true,  :rel => "tooltip", :title => "Mostrar Equipos", :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png'), edit_user_team_path(@user, team),  :remote => true,  :rel => "tooltip", :title => "Editar Equipos",:class => 'btn btn-mini' %>
        <%= link_to image_tag('eliminar.png'), user_team_path(@user, team), :data => { :confirm => 'Esta Seguro?' }, :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger',  :rel => "tooltip", :title => "Eliminar Equipos" %>
    </td>
</tr>

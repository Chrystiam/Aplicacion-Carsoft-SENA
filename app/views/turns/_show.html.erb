<%- model_class = Turn -%>
	<div class="page-header">
  		<h1>Turnos</h1>
		<table class="table table-bordered table-hover" >
	    	<caption>
	    		<h3>Informacion</h3>
	    	</caption>
	    	<thead>
	    		<tr class="success">
	    			<th>Id</th>
	    			<th>Fecha</th>
	    		</tr>
	    	</thead>
	    	<tbody>
	    		<tr class="warning">
				    <td><p><%= @turn.id %></p></td>
				    <td><p><%= @turn.date %></p></td> 
	    		</tr>
	    	</tbody>
	    </table>
	</div>

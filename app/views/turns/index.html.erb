<%- model_class = Turn -%>
  <div class="page-header">
    <h1>Turnos</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>Id</th>
          <th>Fechas</th>
        </tr>
      </thead>
      <tbody>
       <%= render @turns %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_turn_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
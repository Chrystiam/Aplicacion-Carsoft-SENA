<%= simple_form_for(@type_material, :html => { :class => 'form-horizontal' }, :remote => true) do |f| %>
	    <%= f.error_notification %>
	    <%= display_base_errors @type_material %>

	  <p><%= f.input :name %></p>
	  <p><%= f.input :description %></p>
 
  
    <div class="form-actions">
      <%= f.submit "guardar", :class => 'btn btn-primary' %>
      <%= link_to 'Atras', type_materials_path, :class => 'btn btn-small' %>
    </div>
<% end %>

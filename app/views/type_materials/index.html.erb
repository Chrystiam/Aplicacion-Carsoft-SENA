<%- model_class = TypeMaterial -%>
  <div class="page-header">
    <h1>Tipo Materiales</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>Id</th>
          <th>Nombres</th>
          <th>Descripcion</th>
        </tr>
      </thead>
      <tbody>
       <%= render @type_materials %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_type_material_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>
<%- model_class = TypeVehicle -%>
<div class="page-header">
  <h1>Tipos Vehiculos</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>description</th>
      <th>created_at</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @type_vehicles.each do |type_vehicle| %>
      <tr>
        <td><%= link_to type_vehicle.id, type_vehicle_path(type_vehicle) %></td>
        <td><%= type_vehicle.name %></td>
        <td><%= type_vehicle.description %></td>
        <td><%=l type_vehicle.created_at %></td>
        <td>
          <%= link_to 'Editar', edit_type_vehicle_path(type_vehicle), :class => 'btn btn-mini' %>
          <%= link_to 'Eliminar', type_vehicle_path(type_vehicle),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Nuevo Tipo Vehiculo', new_type_vehicle_path, :class => 'btn btn-primary' %>

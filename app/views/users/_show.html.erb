<%- model_class = User -%>
	<div class="page-header">
  		<h1>Repuestas</h1>
		<table class="table table-bordered table-hover" >
	    	<caption>
	    		<h3>Informacion</h3>
	    	</caption>
	    	<thead>
	    		<tr class="success">
	    			  <th>id</th>
			          <th>Nombres</th>
			          <th>Apellidos</th>
			          <th>Responsible</th>
			          <th>Identificaciones</th>
			          <th>emails</th>
			          <th>Dirreciones</th>
			          <th>Telefonos</th>
			          <th>Celulars</th>
	    		</tr>
	    	</thead>
	    	<tbody>
	    		<tr class="warning">
	    			<td><p><%= image_tag @user.avatar.url(:thumb) %></p></td>
	    			<td><p><%= @user.id  %></p></td>
				    <td><p><%= @user.name  %></p></td>
				    <td><p><%= @user.surname %></p></td>
				    <td><p><%= @user.responsible_id %></p></td>
				    <td><p><%= @user.identification %></p></td>
				    <td><p><%= @user.email %></p></td>
				    <td><p><%= @user.address_residence %></p></td>
				    <td><p><%= @user.phone %></p></td>
				    <td><p><%= @user.movil  %></p></td> 
	    		</tr>
	    	</tbody>
	    </table>
	</div>

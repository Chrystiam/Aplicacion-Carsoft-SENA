<tr id=<%= "user_#{user.id}" %>>
    <td><%= link_to user.id, edit_user_path(user), :remote => true %></td>
    <td><%= user.name %></td>
    <td><%= user.surname %></td>
    <td><%= user.identification %></td>
    <td><%= user.email %></td>
    <td><%= user.address_residence %></td>
    <td><%= user.phone %></td>
    <td><%= user.movil %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), user,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_user_path(user),  :remote => true,
          :class => 'btn btn-mini'%>
        <%= link_to image_tag('eliminar.png', :title => "Eliminar Equipos"), user, :data => { :confirm => 'Esta Seguro?' },
          :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
           <td><%= link_to image_tag('vehicles.png', :tile => "Agregar cursos"), user_vehicles_path(user) %></td>
         
    </td>
</tr>

<%- model_class = Vehicle -%>
  <div class="page-header">
    <h1>Vehiculos</h1>
  </div>
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>Id</th>
          <th>Placa</th>
          <th>color</th>
          <th>Marca</th>
          <th>Modelo</th>
          <th>Tipo vehiculo</th>
          <th>Usuario</th>
        </tr>
      </thead>
      <tbody>
       <%= render @vehicles %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png', :tile  => "Mostrar Equipos"),  new_vehicle_path,  :remote => true %></td>
        </tr>
      </tfoot>
    </table>